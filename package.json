{
  "name": "natural-ai-desktop",
  "version": "1.0.0",
  "description": "Natural AI Desktop UI built with Electron, React, TypeScript, and Tailwind CSS",
  "main": "dist/electron/main.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:react\" \"npm run dev:electron\"",
    "dev:react": "vite",
    "dev:electron": "wait-on http://localhost:3000 && tsc -p tsconfig.electron.json && cross-env NODE_ENV=development electron .",
    "build": "tsc && vite build && tsc -p tsconfig.electron.json",
    "start": "electron ."
  },
  "keywords": [
    "electron",
    "react",
    "typescript",
    "tailwind"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^28.0.0",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.3.3",
    "vite": "^5.0.8",
    "wait-on": "^7.2.0"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "framer-motion": "^12.34.3",
    "lucide-react": "^0.294.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^3.5.0"
  }
}
